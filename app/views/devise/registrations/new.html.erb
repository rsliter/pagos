<h2>Registrarse</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :first_name, "Nombre" %><br />
  <%= f.text_field :first_name, :autofocus => true %></div>

  <div><%= f.label :last_name, "Apellido" %><br />
  <%= f.text_field :last_name, :autofocus => true %></div>

  <div><%= f.label :email, "Correo electronico" %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :password, "Clave" %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation, "ConfirmaciÃ³n de clave" %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Registrarse" %></div>
<% end %>
